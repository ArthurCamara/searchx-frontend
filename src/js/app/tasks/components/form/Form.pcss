@import '../../../Variables.pcss';


    .Form {
        background-color: $primary-color;
        
        .body {
            background-color: #ffffff;
            max-width: 720px;
            min-height: 100vh;
            margin: 0 auto;
            display:flex; flex-direction:row;
            
            .Side {
                width: 400px;
                padding-left: 20px;
                padding-right: 20px;
                padding-bottom: 20px;
                margin-top: 20px;

                h3 {
                font-size: 20px;

            }
        }
            .message {
                padding: 10px 20px 20px;
            }

            .panel-body {
                padding: 10px 20px 0;

                .panel-footer {
                    text-align: right;
                    margin: 10px -20px 0;
                }
            }

            .sv_next_btn, .sv_complete_btn {
                background-color: $primary-color;
                color: white;
            }
            
    }

    
    
}